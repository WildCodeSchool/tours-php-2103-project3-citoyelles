<form method="post" action="{{ path('delete_calendar', {'id': calendar.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet évènement?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ calendar.id) }}">
    <button class="btn">Supprimer</button>
</form>
